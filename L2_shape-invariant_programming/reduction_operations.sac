use Array: all;
use SimplePrint: all;

int main()
{
  vect = [1,2,3,4,5,6,7,8,9];

  mat = [ vect, vect+10, vect+20];

  a = print( sum( mat) );
  a+= print( prod( vect) );
  a+= print( Bool::toi( all( mat >= 1) ));
  a+= print( Bool::toi( any( mat > 1) ));
  a+= print( maxval( mat) );
  a+= print( minval( mat) );

  return(a);
}


use Array: all;
use StdIO: all;

int main()
{
  vect = [1,2,3,4,5,6,7,8,9];

  mat = [ vect, vect+10, vect+20];

  print( sum( mat) );
  print( prod( vect) );
  print( Bool::toi( all( mat >= 1) ));
  print( Bool::toi( any( mat > 1) ));
  print( maxval( mat) );
  print( minval( mat) );

  return(0);
}


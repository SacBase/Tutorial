use Array: all;
use Math: all;
use SimplePrint: all;

int main()
{
  vect = [1,2,3,4,5,6,7,8,9];

  mat = [ vect, vect+10, vect+20];

  a = toi( print( sum( mat) ));
  a = toi( print( prod( vect) ));
  a = toi( print( all( mat >= 1) ));
  a = toi( print( any( mat > 1) ));
  a = toi( print( maxval( mat) ));
  a = toi( print( minval( mat) ));

  return(0);
}


use Array: all;
use SimplePrint: all;

int main()
{
  vect = [1,2,3,4,5,6,7,8,9];

  mat = [ vect, vect+10, vect+20];

  a = mat[[]];
  b = print( a);

  a = mat[[.]];
  b+= print( a);

  a = mat[[.,.]];
  b+= print( a);

  a = mat[[...]];
  b+= print( a);

  a = mat[[.,...]];
  b+= print( a);

  a = mat[[...,.]];
  b+= print( a);

  a = mat[[.,.,...]];
  b+= print( a);

  a = mat[[.,...,.]];
  b+= print( a);

  a = mat[[...,.,.]];
  b+= print( a);

  return(b);
}


use Array: all;
use StdIO: all;

int main()
{
  vect = [1,2,3,4,5,6,7,8,9];

  mat = [ vect, vect+10, vect+20];

  a = mat[[]];
  print( a);

  a = mat[[.]];
  print( a);

  a = mat[[.,.]];
  print( a);

  a = mat[[...]];
  print( a);

  a = mat[[.,...]];
  print( a);

  a = mat[[...,.]];
  print( a);

  a = mat[[.,.,...]];
  print( a);

  a = mat[[.,...,.]];
  print( a);

  a = mat[[...,.,.]];
  print( a);

  return(0);
}


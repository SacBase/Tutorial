use Array: all;
use SimplePrint: all;

int main()
{
  vect = [1,2,3,4,5,6,7,8,9];

  mat = [ vect, vect+10, vect+20];
  a = print( mat);

  a+= print( mat[[1]] );
  a+= print( mat[[1,.]] );
  a+= print( mat[[1,...]] );

  a+= print( mat[[.,1]] );
  a+= print( mat[[...,1]] );

  a+= print( mat[[1,...,1]] );

  arr3d = [ mat, mat];
  a+= print( arr3d);

  a+= print( arr3d[[.,1]] );
  a+= print( arr3d[[...,1]] );

  return(a);
}

